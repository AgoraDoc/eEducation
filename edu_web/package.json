{
  "name": "agora-edu-web",
  "productName": "AgoraEduElectron",
  "version": "3.0.0",
  "private": true,
  "homepage": ".",
  "main": "./src/index.tsx",
  "agora_electron": {
    "electron_version": "5.0.8",
    "prebuilt": true,
    "platform": "darwin"
  },
  "build": {
    "extends": "null",
    "productName": "AgoraEducation",
    "appId": "io.agora.education",
    "files": [
      "./node_modules/**/*",
      "./build/**/*",
      "./src/**/*",
      "package.json"
    ],
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis",
        "msi"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "rpm",
        "snap",
        "AppImage"
      ],
      "category": "Development"
    },
    "directories": {
      "buildResources": "resources",
      "output": "release"
    }
  },
  "dependencies": {
    "@material-ui/core": "^4.5.1",
    "@material-ui/icons": "^4.5.1",
    "@netless/react-seek-slider": "^1.0.4",
    "@types/jest": "^24.0.19",
    "@types/lodash": "^4.14.145",
    "@types/node": "^12.11.1",
    "@types/react": "^16.9.9",
    "@types/react-dom": "^16.9.2",
    "@types/react-router-dom": "^5.1.0",
    "agora-electron-sdk": "2.9.1-beta.1",
    "agora-rtc-sdk": "3.0.0",
    "agora-rtm-sdk": "^1.2.0",
    "animate.css": "^3.7.2",
    "concurrently": "^5.0.0",
    "electron-is-dev": "^1.1.0",
    "foreman": "^3.0.1",
    "immutable": "^4.0.0-rc.12",
    "js-md5": "^0.7.3",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "node-sass": "^4.12.0",
    "react": "^16.10.2",
    "react-color": "^2.17.3",
    "react-dom": "^16.10.2",
    "react-draggable": "^4.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "rxjs": "^6.5.3",
    "typescript": "^3.6.4",
    "wait-on": "^3.3.0"
  },
  "scripts": {
    "electron": "cross-env REACT_APP_RUNTIME_PLATFORM=electron ELECTRON_START_URL=http://localhost:3000 concurrently \"npm run electron:web\" \"wait-on http://localhost:3000 && npm run electron:client\"",
    "electron:client": "cross-env electron ./src/main/index.js",
    "electron:web": "cross-env BROWSER=none react-app-rewired start",
    "dev:electron": "node scripts/electron.dev",
    "electron:build": "cross-env REACT_APP_RUNTIME_PLATFORM=electron react-app-rewired build",
    "dev": "cross-env REACT_APP_RUNTIME_PLATFORM=web react-app-rewired start",
    "build": "cross-env REACT_APP_RUNTIME_PLATFORM=web react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject",
    "pack": "electron-builder --dir -m"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/ali-oss": "^6.0.4",
    "@types/react-color": "^3.0.1",
    "@types/uuid": "^3.4.6",
    "ali-oss": "^6.3.1",
    "customize-cra": "^0.8.0",
    "electron": "5.0.8",
    "electron-builder": "^21.2.0",
    "react-app-rewired": "^2.1.4",
    "uuid": "^3.3.3",
    "white-web-sdk": "2.3.17"
  }
}
